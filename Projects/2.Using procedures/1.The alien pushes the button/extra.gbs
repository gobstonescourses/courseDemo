/*@ATTRIBUTE@block_name@Move the Alien to the East@*/
procedure MoveTheAlienToTheEast() {
  if (not hayBolitas(Verde)) { BOOM("The head is not on a cell with the Alien") }
  if (not puedeMover(Este))  { BOOM("The Alien crashed against the wall with the button") }
  Sacar(Verde) 
  Mover(Este)
  Poner(Verde)
}

procedure PushButton() { 
  if (not hayBolitas(Rojo)) { BOOM("The Alien is too far from the button") }
  Sacar(Rojo) 
}