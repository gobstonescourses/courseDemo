procedure PonerElCarácter(códigoDelCarácter) {
  LimpiarLaCelda()
  Poner_BolitasNegras(códigoDelCarácter)
  AvanzarElCursor()
}

procedure AvanzarElCursor() {
  if (puedeMover(Este)) {
    Mover(Este)
  } else {
    NuevaLínea()
  }
}

procedure NuevaLínea() {
  MoverSiSePuede(Sur)
  IrAlBorde(Oeste)
}

procedure LimpiarLaCelda() {
  SacarTodasLasBolitasNegras()
}

