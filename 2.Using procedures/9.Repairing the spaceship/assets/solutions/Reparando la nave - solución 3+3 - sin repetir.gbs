/*
*/
program { RepararLaNaveYEscapar() }

procedure RepararLaNaveYEscapar() {
  /*
  */
  ConseguirTodoElHierro()
  ConseguirTodoElCarbón()
  VolverACasa()
}

procedure ConseguirTodoElHierro() { 
  /*
  */
  ConseguirUnHierro()
  ConseguirUnHierro()
  ConseguirUnHierro()
}

procedure ConseguirTodoElCarbón()() { 
  /*
  */
  ConseguirUnCarbón()
  ConseguirUnCarbón()
  ConseguirUnCarbón()
}

procedure ConseguirUnHierro() {
  /*
  */
  IrHastaElHierro()
  AgarrarHierro()
  VolverConElHierro()
  PonerHierroEnLaNave()
}

procedure ConseguirUnCarbón() {
  /*
  */
  IrHastaElCarbón()
  AgarrarCarbón()
  VolverConElCarbón()
  PonerCarbonEnLaNave()
}

procedure IrHastaElHierro() { 
  /*
  */
  MoverMarcianoAl_(Norte)
  MoverMarcianoAl_(Norte)
  MoverMarcianoAl_(Norte)
}

procedure VolverConElHierro() {
  /*
  */
  MoverMarcianoAl_(Sur)
  MoverMarcianoAl_(Sur)
  MoverMarcianoAl_(Sur)
}

procedure IrHastaElCarbón() {
  /*
  */
  MoverMarcianoAl_(Norte); MoverMarcianoAl_(Este)
  MoverMarcianoAl_(Norte); MoverMarcianoAl_(Este)
  MoverMarcianoAl_(Norte); MoverMarcianoAl_(Este)
  MoverMarcianoAl_(Este)
}

procedure VolverConElCarbón() {
  /*
  */
  MoverMarcianoAl_(Sur); MoverMarcianoAl_(Oeste)
  MoverMarcianoAl_(Sur); MoverMarcianoAl_(Oeste)
  MoverMarcianoAl_(Sur); MoverMarcianoAl_(Oeste)
  MoverMarcianoAl_(Oeste)
}
