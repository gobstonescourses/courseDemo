program{
    BuscarLaLlave()
    BuscarElSombrero()
    ConseguirLaEspada()
    RescatarAlNovio()
    BusarElUnicornioYEscapar()
}

procedure IrALaLlave() {
    MoverProtagonistaAl_(Norte) 
    MoverProtagonistaAl_(Norte)
    MoverProtagonistaAl_(Norte)
    MoverProtagonistaAl_(Este)
    MoverProtagonistaAl_(Este)
    MoverProtagonistaAl_(Este)
    MoverProtagonistaAl_(Este)
}

procedure BuscarLaLlave() {
    IrALaLlave()
    AgarrarLaLlave()
}

procedure IrAlCofre() {
